{
  "scripts": {
    "lokalise": "npx ts-node -r dotenv/config --project ./scripts/tsconfig.json --transpile-only ./scripts/lokalise/sync.ts",
    "format": "prettier --loglevel error --write \"{cypress}/**/*.{ts,tsx,js,json}\"",
    "format:check": "prettier --loglevel error --check \"{cypress}/**/*.{ts,tsx,js,json}\"",
    "lint": "npx eslint . --ext .ts",
    "e2e": "cypress open",
    "e2e:headless": "cypress run --browser chrome",
    "cy:parallel": "cypress-parallel -s e2e:headless -t 4 -d 'cypress/e2e/smoke/' -m false",
    "cy:smoke": "set CYPRESS_INCLUDE_TAGS=SIGNUP,CYPRESS_INCLUDE_USE_BOOLEAN_AND=true && cypress run",
    "e2e:dashboard": "cypress run --record --key e31ef6cc-ded3-48e8-8813-540383fbfaa3 --browser chrome",
    "e2e-parallel:dashboard": "cypress run --record --key e31ef6cc-ded3-48e8-8813-540383fbfaa3 --parallel --ci-build-id $BUILD_BUILDNUMBER --browser chrome --group 'Azure CI - PAM - PJ'",
    "e2e-parallel:dashboard:percy": "npx percy exec --config .percy.yml  -- cypress run --record --key e31ef6cc-ded3-48e8-8813-540383fbfaa3 --parallel --ci-build-id $BUILD_BUILDNUMBER --browser chrome --group 'Azure CI - PAM - PJ'"
  },
  "devDependencies": {
    "@lokalise/node-api": "^9.2.0",
    "@percy/cli": "^1.10.4",
    "@percy/cypress": "^3.1.2",
    "@testing-library/cypress": "^9.0.0",
    "@types/auth0-js": "^9.14.6",
    "@types/faker": "5.5.8",
    "@types/fs-extra": "^9.0.13",
    "@types/imap-simple": "^4.2.5",
    "@types/nodemailer": "^6.4.4",
    "@types/tedious": "^4.0.7",
    "@typescript-eslint/parser": "^5.30.0",
    "adm-zip": "^0.5.9",
    "auth0-js": "^9.19.0",
    "cypress": "^12.7.0",
    "cypress-mochawesome-reporter": "^3.2.2",
    "cypress-multi-reporters": "^1.6.1",
    "cypress-network-idle": "^1.11.0",
    "cypress-parallel": "^0.12.0",
    "cypress-recurse": "^1.23.0",
    "cypress-tags": "^1.1.2",
    "deepmerge": "^4.2.2",
    "dotenv": "^16.0.1",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-chai-friendly": "^0.7.2",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-prettier": "^4.1.0",
    "faker": "6.6.6",
    "flat": "^5.0.2",
    "fs-extra": "^10.1.0",
    "futoin-hkdf": "^1.5.0",
    "got": "11.8.2",
    "imap-simple": "^5.1.0",
    "jose": "^4.10.0",
    "mailparser": "^3.5.0",
    "mocha-junit-reporter": "^2.1.0",
    "node-lokalise-api": "^1.0.0",
    "nodemailer": "^6.7.7",
    "prettier": "^2.7.1",
    "tedious": "^15.1.0",
    "typescript": "^4.8.4"
  }
}
